{
  "extends": "airbnb",
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.client.prod.js"
      }
    }
  },
  "rules": {
    "brace-style": 0,
    "camelcase": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "global-require": 0,
    "import/extensions": 0,
    "max-len": 0,
    "new-cap": 0,
    "no-console": 0,
    "no-else-return": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "prefer-const": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-indent": 1,
    "react/prop-types": 0
  }
}
